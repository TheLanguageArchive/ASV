<?xml version="1.0" encoding="UTF-8"?>
<Context antiJARLocking="true" path="/metadata-browser-pages">
  <Parameter description="Default Spring profile, may be overridden using spring.profiles.active" name="spring.profiles.default" value="cmdi-production"/>
  <!-- 
	Spring profile for runtime. Actual beans may come from dependency, also see contextConfigLocation parameter in web.xml. 

	Profiles:
	    * cmdi-production: combines all production profiles
	     * cmdi-production-providers: only the metadata browser providers/services beans
	     * cmdi-production-csdb: only the corpus structure database beans
	    
	    * cmdi-adapter: combines all adapter profiles
	     * cmdi-adapter-csdb: adapter implementation of corpus structure provider that connects to the 'old' (IMDI) corpus structure API
	     * cmdi-adapter-ams: adapter implementation of License and Authorization services that wrap 'old' AMS services
	    
	    * cmdi-mock-csdb: mock implementation of CMDI CSDB
	    * cmdi-mock-ams: mock implementation of AMS License and Authorization services
    -->
  <Parameter name="spring.profiles.active" value="cmdi-production-providers, cmdi-adapter-csdb, cmdi-mock-ams"/>
</Context>
